# Google Cloud Platform Configuration
# Your GCP project ID (e.g., my-project-123)
GOOGLE_CLOUD_PROJECT=your-project-id

# Path to your GCP service account key file
# Download from GCP Console -> IAM & Admin -> Service Accounts -> Create Key
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account-key.json

# LangGraph Configuration
# Your LangGraph API key from your account settings
LANGGRAPH_API_KEY=your-langgraph-api-key

# Application Configuration
# Set to False in production
DEBUG=True

# Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Server Configuration
# Port for local development server
PORT=8080

# Host for local development server
HOST=localhost

# New additions from the code block
OPENAI_API_KEY=sk-proj-_...
LANGCHAIN_API_KEY=lsv2_pt_...
LANGGRAPH_CLOUD_LICENSE_KEY=lsv2_pt_...
LANGSMITH_API_KEY=lsv2_pt_... 